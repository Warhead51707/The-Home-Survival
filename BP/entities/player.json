{
	"format_version": "1.18.10",
	"minecraft:entity": {
		"description": {
			"identifier": "minecraft:player",
			"is_spawnable": false,
			"is_summonable": false,
			"is_experimental": false
		},
		"component_groups": {
			"home:speed_for_u": {
				"minecraft:movement": {
					"value": 0.12
				}
			},
			"home:health_for_u": {
				"minecraft:health": {
					"value": 40,
					"max": 40
				}
			},
			"home:default_stats": {
				"minecraft:health": {
					"value": 20,
					"max": 20
				}
			}
		},
		"components": {
			"minecraft:experience_reward": {
				"on_death": "Math.Min(query.player_level * 7, 100)"
			},
			"minecraft:type_family": {
				"family": [
					"player"
				]
			},
			"minecraft:is_hidden_when_invisible": {},
			"minecraft:loot": {
				"table": "loot_tables/empty.json"
			},
			"minecraft:collision_box": {
				"width": 0.6,
				"height": 1.8
			},
			"minecraft:can_climb": {},
			"minecraft:movement": {
				"value": 0.1
			},
			"minecraft:hurt_on_condition": {
				"damage_conditions": [
					{
						"filters": {
							"test": "in_lava",
							"subject": "self",
							"operator": "==",
							"value": true
						},
						"cause": "lava",
						"damage_per_tick": 4
					}
				]
			},
			"minecraft:health": {
				"value": 20,
				"max": 20
			},
			"minecraft:attack": {
				"damage": 1
			},
			"minecraft:player.saturation": {
				"value": 20
			},
			"minecraft:player.exhaustion": {
				"value": 0,
				"max": 4
			},
			"minecraft:player.level": {
				"value": 0,
				"max": 24791
			},
			"minecraft:player.experience": {
				"value": 0,
				"max": 1
			},
			"minecraft:breathable": {
				"total_supply": 15,
				"suffocate_time": -1,
				"inhale_time": 3.75,
				"generates_bubbles": false
			},
			"minecraft:nameable": {
				"always_show": true,
				"allow_name_tag_renaming": false
			},
			"minecraft:physics": {},
			"minecraft:pushable": {
				"is_pushable": false,
				"is_pushable_by_piston": true
			},
			"minecraft:insomnia": {
				"days_until_insomnia": 3
			},
			"minecraft:damage_sensor": {
				"triggers": [
					{
						"cause": "entity_explosion",
						"on_damage": {
							"filters": {
								"test": "is_family",
								"subject": "other",
								"value": "land_mine"
							}
						},
						"deals_damage": false
					},
					{
						"cause": "all",
						"on_damage": {
							"filters": {
								"test": "is_family",
								"subject": "other",
								"value": "turret"
							}
						},
						"deals_damage": false
					}
				]
			},
			"minecraft:rideable": {
				"seat_count": 2,
				"family_types": [
					"parrot_tame"
				],
				"pull_in_entities": true,
				"seats": [
					{
						"position": [
							0.4,
							-0.2,
							-0.1
						],
						"min_rider_count": 0,
						"max_rider_count": 0,
						"lock_rider_rotation": 0
					},
					{
						"position": [
							-0.4,
							-0.2,
							-0.1
						],
						"min_rider_count": 1,
						"max_rider_count": 2,
						"lock_rider_rotation": 0
					}
				]
			},
			"minecraft:environment_sensor": {
				"triggers": [
					{
						"event": "home:add_health_for_u",
						"target":"self",
						"filters": {
							"test": "has_tag",
							"operator": "==",
							"value": "healthforu"
						}
					},
					{
						"event": "home:remove_health_for_u",
						"target":"self",
						"filters": {
							"test": "has_tag",
							"operator": "!=",
							"value": "healthforu"
						}
					},
					{
						"event": "home:speed_for_u",
						"target": "self",
						"filters": {
							"test": "has_tag",
							"value": "speedforu"
						}
					}
				]
			},
			"minecraft:conditional_bandwidth_optimization": {},
			"minecraft:block_climber": {}
		},
		"events": {
			"home:speed_for_u": {
				"add": {
					"component_groups": [
						"home:speed_for_u"
					]
				}
			},
			"home:add_health_for_u": {
				"add": {
					"component_groups": [
						"home:health_for_u"
					]
				},
				"remove": {
					"component_groups": [
						"home:default_stats"
					]
				}
			},
			"home:remove_health_for_u": {
				"add": {
					"component_groups": [
						"home:default_stats"
					]
				},
				"remove": {
					"component_groups": [
						"home:health_for_u"
					]
				}
			}
		}
	}
}